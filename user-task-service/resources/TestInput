curl -i -H "Content-Type: application/json" -X POST -d '{"name":"My task4","description" : "Description of task", "date_time" : "2016-05-25 14:25:00"}' http://localhost:8080/api/user/1/task
curl -i -H "Content-Type: application/json" -X PUT -d  '{"name":"My updated task"}' http://localhost:8080/api/user/1/task/1
curl -i -H "Accept: application/json" -H "Content-Type: application/json" -X GET http://localhost:8080/api/user/1/task/1 
curl -i -H "Accept: application/json" -H "Content-Type: application/json" -X GET http://localhost:8080/api/user/1/task 
curl -i -H "Content-Type: application/json" -X DELETE http://localhost:8080/api/user/1/task/4

curl -i -H "Content-Type: application/json" -X PUT -d '{"status":"PENDING"}' http://localhost:8080/api/user/1/task/1
