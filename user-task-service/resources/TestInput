
curl -i -H "Content-Type: application/json" -X POST -d '{"username":"testUser1","first_name" : "John",  "last_name" : "Smith"}' http://localhost:8080/api/user
curl -i -H "Content-Type: application/json" -X POST -d '{"username":"testUser2","first_name" : "Jerry", "last_name" : "Smith"}' http://localhost:8080/api/user
curl -i -H "Content-Type: application/json" -X POST -d '{"username":"testUser3","first_name" : "Jonny", "last_name" : "Smith"}' http://localhost:8080/api/user
curl -i -H "Content-Type: application/json" -X POST -d '{"username":"testUser4","first_name" : "Amber", "last_name" : "Smith"}' http://localhost:8080/api/user
curl -i -H "Content-Type: application/json" -X PUT -d '{"first_name" : "John", "last_name" : "Doe"}' http://localhost:8080/api/user/1
curl -i -H "Accept: application/json" -H "Content-Type: application/json" -X GET http://localhost:8080/api/user
curl -i -H "Accept: application/json" -H "Content-Type: application/json" -X GET http://localhost:8080/api/user/1 
curl -i -H "Content-Type: application/json" -X POST -d '{"name":"My task1","description" : "Description of task", "date_time" : "2016-05-25 14:25:00"}' http://localhost:8080/api/user/1/task
curl -i -H "Content-Type: application/json" -X POST -d '{"name":"My task2","description" : "Description of task", "date_time" : "2016-05-25 14:25:00"}' http://localhost:8080/api/user/1/task
